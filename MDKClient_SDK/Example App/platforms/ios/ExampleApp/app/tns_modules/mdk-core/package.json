{
  "name": "mdk-core",
  "version": "5.2.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean-install-module": "./bin/clean_install_module.sh",
    "checkall": "npm run lint && npm test && ./bin/run_all_tests.sh",
    "eslint": "eslint 'src/tests/**/*.spec.js'",
    "eslint-fix": "eslint 'src/tests/**/*.spec.js' --fix",
    "eslint_report": "eslint -f checkstyle 'src/tests/**/*.spec.js' > checkstyle_eslint.xml",
    "tslint": "gulp lint_verbose",
    "lint": "npm run tslint && npm run eslint",
    "test": "nyc --reporter=lcov nyc --reporter=html --reporter=text --reporter=text-summary gulp --max_old_space_size=12288 test",
    "test-unit": "nyc --reporter=lcov nyc --reporter=html --reporter=text --reporter=text-summary gulp --max_old_space_size=12288 unit",
    "test-coverage": "nyc --reporter=lcov nyc --reporter=html --reporter=text --reporter=text-summary --reporter=cobertura gulp --max_old_space_size=12288 test_coverage && mv -f ./coverage/cobertura-coverage.xml ./coverage/cobertura-ts-coverage.xml",
    "test-no-cover": "./bin/run_all_tests.sh",
    "build": "gulp --max_old_space_size=12288 build",
    "compile": "gulp --max_old_space_size=12288 compile",
    "clean": "gulp clean"
  },
  "devDependencies": {
    "@nativescript-rtl/ui": "0.1.6",
    "@nativescript/core": "6.5.27",
    "@nota/nativescript-accessibility-ext": "6.5.1",
    "@nstudio/nativescript-pulltorefresh": "2.0.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "del": "5.0.0",
    "eslint": "3.13.1",
    "eslint-config-google": "0.7.1",
    "eslint-plugin-chai-expect": "1.1.1",
    "eslint-plugin-mocha": "4.8.0",
    "gulp": "4.0.2",
    "gulp-load-plugins": "2.0.1",
    "gulp-mocha": "7.0.2",
    "gulp-sourcemaps": "2.6.5",
    "gulp-tslint": "7.0.1",
    "gulp-tslint-jenkins-reporter": "1.2.0",
    "gulp-txtjs": "0.0.4",
    "gulp-typescript": "5.0.1",
    "gulp-watch": "5.0.1",
    "mdk-sap": "file:modules/SAP",
    "mocha": "6.2.0",
    "nativescript-ui-sidedrawer": "8.0.1",
    "nyc": "15.0.1",
    "proxyquire": "1.7.10",
    "sinon": "7.4.1",
    "tns-core-modules": "6.5.27",
    "tns-platform-declarations": "6.5.15",
    "toolbar-plugin": "file:modules/toolbar-plugin",
    "tslib": "2.0.3",
    "tslint": "6.1.1",
    "typescript": "3.6.3",
    "zip-plugin": "file:modules/zip-plugin"
  },
  "dependencies": {
    "crypto-js": "3.1.9-1",
    "rtl-detect": "1.0.2",
    "url": "0.11.0"
  },
  "peerDependencies": {
    "crypto-js": "3.1.9-1",
    "rtl-detect": "1.0.2",
    "url": "0.11.0"
  }
}
